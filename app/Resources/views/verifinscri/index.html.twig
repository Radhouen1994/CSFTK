{% extends 'admin.html.twig' %}

{% block body %}
    <div class="col-lg-6 col-lg-offset-1">
    <h1>VerifInscri list</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Vercin</th>
                <th>Vernomdoss</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for verifInscri in verifInscris %}
            <tr>
                <td>{{ verifInscri.verCin }}</td>
                <td>{{ verifInscri.verNomdoss }}</td>
                <td>
                    <ul class="list-unstyled">
                        <li>
                            <a href="{{ path('verifinscri_show', { 'id': verifInscri.idverifInscri }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('verifinscri_edit', { 'id': verifInscri.idverifInscri }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul class="list-unstyled">
        <li>
            <a href="{{ path('verifinscri_new') }}">Create a new entry</a>
        </li>
    </ul>
    </div>
{% endblock %}
